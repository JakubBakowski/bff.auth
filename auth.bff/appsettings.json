{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "KeyVault": {
    "VaultUri": "https://your-key-vault.vault.azure.net/",
    "TenantId": "your-tenant-id",
    "ClientId": "your-client-id",
    "ClientSecret": "your-client-secret"
  },
  "AzureBlobStorage": {
    "ConnectionString": "DefaultEndpointsProtocol=https;AccountName=your-account;AccountKey=your-key;EndpointSuffix=core.windows.net",
    "ContainerName": "data-protection-keys",
    "BlobName": "keys.xml"
  },
  "Authentication": {
    "Authority": "https://your-identity-provider",
    "ClientId": "your-client-id",
    "ClientSecret": "your-client-secret"
  },
  "ReverseProxy": {
    "Routes": {
      "api": {
        "ClusterId": "apiCluster",
        "Match": {
          "Path": "/api/{**catch-all}"
        }
      },
      "spa": {
        "ClusterId": "spaCluster",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    },
    "Clusters": {
      "apiCluster": {
        "Destinations": {
          "api1": {
            "Address": "http://localhost:5001"
          }
        }
      },
      "spaCluster": {
        "Destinations": {
          "spa1": {
            "Address": "http://localhost:5002"
          }
        }
      }
    }
  }
}
